<div>
    <form [formGroup]="form">
    {% for property in entity.properties %}
    {% if get_property_type(property.type) != get_property_type(entity) %}
        <br/>
        <label>{{property.name | capitalize}}</label>
    {% endif %}
        <br/>
		<input class="form-control" type="{{ property | get_form_input_type }}" formControlName={{property.name}}>
        <br/>
	{% endfor %}
    <input class="btn btn-primary" type="submit"
			[disabled]="form.invalid" (click)="Submit()" />
    </form>
</div>
