<br>
<h4 style="text-align: center;">Dashboard</h4>
<div class="container mx-auto mt-5" style="width:75%; text-align: center;">
<br>
{% for entity_row in entities | batch(3, '') %}
<div class="row" >
    {% for entity in entity_row %}
    {% if entity != '' %}
        <div class="card" style="width:20%; height: 140px; margin-bottom: 5%; margin-left: 5%; cursor: pointer;" routerLink="/{{entity.name|format_file_name}}">
            <div class="card-body mt-4" style="font-weight: bold; text-align: center;">
                {{ entity }}
            </div>
        </div>
    {% endif %}
    {% endfor %}
</div>
{% endfor %}
</div>