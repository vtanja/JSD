project: Biblioteka
group: "uns.ac.rs"
description: "Aplikacija koja omogucava rad bibliotekarima"

entity Book:
    title: string;
    publishingYear: int;
    genre: Genre[];
    author: Author;

entity Author:
    firstName: string;
    lastName: string;
    books: Book[];

entity Genre:
    name: string;
    books: Book[];
;
