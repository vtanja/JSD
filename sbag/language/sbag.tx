
SbagModel:
    config=Config?
    simple_types*=BaseType
    entities+=Entity
;

Entity:
    'entity' name=ID ':'
    properties+=EntityProperty
;

EntityProperty:
    Property | Association
;

Property:
    name=ID ':' type=[Type] ';'
;

Association:
    name=ID ':' type=[Entity] '[]' ';'
;

Type:
    BaseType | Entity
;

BaseType:
    'type' name=ID
;

Config:
    'project' ':' project=ID
    'group' ':' group=STRING
    'description' ':' description=STRING
;

Comment:
    /\/\/.*$/
;
